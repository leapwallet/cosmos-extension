    ],
    "reporters": [
      "default",
      "jest-puppeteer-istanbul/lib/reporter"
    ],
    "setupFilesAfterEnv": [
      "jest-puppeteer-istanbul/lib/setup"
    ],
    "coverageDirectory": "coverage"
