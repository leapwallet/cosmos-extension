  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 90000,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/pages/**/*"
    ],
    "reporters": [
      "default",
      "jest-puppeteer-istanbul/lib/reporter"
    ],
    "setupFilesAfterEnv": [
