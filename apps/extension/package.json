{
  "name": "@leap-cosmos/extension",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "build:_production": "cross-env NODE_ENV=production rsbuild build",
    "build:_compass": "cross-env NODE_ENV=production APP=compass rsbuild build",
    "build:_canary": "cross-env NODE_ENV=canary rsbuild build",
    "build:_canary-compass": "cross-env NODE_ENV=canary APP=compass rsbuild build",
    "build:_staging": "cross-env NODE_ENV=staging rsbuild build",
    "build:_staging-compass": "cross-env NODE_ENV=staging APP=compass rsbuild build",
    "build": "run-p typecheck build:_production",
    "build:compass": "run-p typecheck build:_compass",
    "build:staging": "run-p typecheck build:_staging",
    "build:compass-staging": "run-p typecheck build:_staging-compass",
    "build:canary": "run-p typecheck build:_canary",
    "build:canary-compass": "run-p typecheck build:_canary-compass",
    "start:_development": "cross-env NODE_ENV=development rsbuild dev",
    "start:_dev:compass": "cross-env NODE_ENV=development APP=compass rsbuild dev",
    "start:compass": "run-p typecheck:watch start:_dev:compass",
    "start": "run-p typecheck:watch  start:_development",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prettier:check": "prettier -c ./src",
    "prettier:write": "prettier -w ./src",
    "eslint:lint": "eslint ./src ",
    "check": "yarn prettier:check && yarn eslint:lint",
    "eslint:fix": "eslint --fix ./src",
    "test:e2e": "NODE_OPTIONS=\"--max_old_space_size=4096\" yarn jest --coverage --runInBand --detectOpenHandles e2e-tests",
    "test": "yarn test:e2e"
  },
